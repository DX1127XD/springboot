<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>indexServlet</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>confirm</servlet-name>
        <servlet-class>servlet.confirm</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>confirm</servlet-name>
        <url-pattern>/confirm</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RegistServlet</servlet-name>
        <servlet-class>servlet.RegistServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>RegistServlet</servlet-name>
        <url-pattern>/RegistServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>SaveServlet</servlet-name>
        <servlet-class>servlet.SaveServlet</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>SaveServlet</servlet-name>
        <url-pattern>/SaveServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>delSelectedServlet</servlet-name>
        <servlet-class>servlet.delSelectedServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>delSelectedServlet</servlet-name>
        <url-pattern>/delSelectedServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>indexServlet</servlet-name>
        <servlet-class>servlet.indexServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>indexServlet</servlet-name>
        <url-pattern>/indexServlet</url-pattern>
    </servlet-mapping>
</web-app>